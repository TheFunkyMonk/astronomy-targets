# Astronomy API Credentials
ASTRONOMY_API_APP_ID=your_app_id_here
ASTRONOMY_API_APP_SECRET=your_app_secret_here

# Observer Location
LATITUDE=47.6693552
LONGITUDE=-122.3891101
ELEVATION=15.24

# Telescope Configuration
# Options: entry, intermediate, advanced
# - entry: 60-80mm aperture, can see up to magnitude 10
# - intermediate: 100-150mm aperture, can see up to magnitude 12
# - advanced: 200mm+ aperture, can see up to magnitude 14
TELESCOPE_LEVEL=entry

# Evening Viewing Hours (24-hour format)
# Default: 9pm to 2am
EVENING_START_HOUR=21
EVENING_END_HOUR=2
